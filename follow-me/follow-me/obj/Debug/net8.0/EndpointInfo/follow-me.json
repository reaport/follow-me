{
  "openapi": "3.0.1",
  "info": {
    "title": "Follow Me API",
    "description": "API для управления машинами сопровождения.",
    "version": "v1"
  },
  "paths": {
    "/admin": {
      "get": {
        "tags": [
          "Admin"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/admin/cars": {
      "get": {
        "tags": [
          "Admin"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/admin/cars/add": {
      "post": {
        "tags": [
          "Admin"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/admin/logs": {
      "get": {
        "tags": [
          "Admin"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/admin/audit": {
      "get": {
        "tags": [
          "Admin"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/get_car": {
      "post": {
        "tags": [
          "FollowMe"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WeNeedFollowMeRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/WeNeedFollowMeRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/WeNeedFollowMeRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/navigate": {
      "post": {
        "tags": [
          "FollowMe"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NavigationRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/NavigationRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/NavigationRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NavigationRequestDto": {
        "type": "object",
        "properties": {
          "navigate": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WeNeedFollowMeRequestDto": {
        "type": "object",
        "properties": {
          "airplaneId": {
            "type": "string",
            "nullable": true
          },
          "nodeFrom": {
            "type": "string",
            "nullable": true
          },
          "nodeTo": {
            "type": "string",
            "nullable": true
          },
          "isTakeoff": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
  }
}